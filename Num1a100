package nashe;

public class Program {
	public static void main(String[] args) {
		int num = 0 ;
		for (int i=0; i<100; i++)
        {
            if (num % 2 == 0)
            {System.out.println (num + " es par");}
            else 
            {System.out.println(num);}
            num++;
        }
	}
}
